# Configurações do Servidor
PORT=5000
NODE_ENV=development

# Banco de Dados PostgreSQL
DATABASE_URL=postgresql://username:password@localhost:5432/deriv_bots_db

# JWT
JWT_SECRET=sua_chave_secreta_jwt_muito_segura_aqui
JWT_EXPIRES_IN=24h

# Deriv API
DERIV_APP_ID=seu_app_id_deriv
DERIV_OAUTH_URL=https://oauth.deriv.com/oauth2/authorize
DERIV_API_URL=https://ws.binaryws.com/websockets/v3
DERIV_APP_TOKEN=seu_token_deriv

# Upload
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760

# Cors
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Admin Default
ADMIN_EMAIL=admin@derivbots.com
ADMIN_PASSWORD=admin123456 